{
  "luci-app-tfortis-https": {
    "description": "Grant UCI access to LuCI app tfortis-https",
    "read": {
      "uci": [
        "https"
      ],
      "file": {
        "/etc/nginx/conf.d/": [ "list" ]
      }
    },
    "write": {
      "uci": [
        "https"
      ],
      "file": {
        "/etc/nginx/conf.d/*": [ "write" ]
      }
    }
  }
}
