
config lldpd 'config'
	option enable_cdp '1'
	option enable_fdp '1'
	option enable_sonmp '1'
	option enable_edp '1'
	option agentxsocket '/var/run/agentx.sock'
	option lldp_class '4'
	option lldp_location '2:FR:6:Commercial Rd:3:Roseville:19:4'
	option enabled '1'
	option lldp_tx_interval '6'
	option lldp_tx_hold '3'
	option readonly_mode '0'
	option lldp_portidsubtype 'macaddress'
	option lldp_agenttype 'nearest-bridge'
	option cdp_version 'cdpv1v2'
	option enable_lldp '1'
	option force_lldp '1'
	list interface 'lan2'
	list interface 'lan3'
	list interface 'lan1'

